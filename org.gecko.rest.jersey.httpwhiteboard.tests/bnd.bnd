# This is the version of JUnit that will be used at build time and run time
junit: org.apache.servicemix.bundles.junit;version="[4.11,5)"

# This is the version of Mockito that will be used at build time and run time
mockito: org.mockito.mockito-core;version="[1.9,2)",\
  org.objenesis;version="[2.1,3)"

# Discover and run all test cases annotated with the @RunWith annotation
#Test-Cases: ${classes;CONCRETE;ANNOTATED;org.junit.runner.RunWith}

# Build dependencies 
-buildpath: \
	${junit},\
	${mockito},\
	osgi.annotation;version='7.0.0',\
	osgi.core;version='7.0',\
	osgi.cmpn;version='7.0',\
	org.gecko.rest.jersey.api;version=latest,\
	org.gecko.rest.jersey.tests;version=latest,\
	org.gecko.util.test.common;version='1.3',\
	jakarta.ws.rs-api;version='2.1'

-runrequires: \
	bnd.identity;version='latest';id='org.gecko.rest.jersey.httpwhiteboard.tests',\
	bnd.identity;id='org.gecko.rest.jersey.api',\
	bnd.identity;id='org.gecko.rest.jersey.impl',\
	bnd.identity;version='2.32.0';id='org.glassfish.jersey.inject.jersey-hk2',\
	bnd.identity;version='1.2.1';id='jakarta.activation-api',\
	bnd.identity;version='2.0.2';id='jakarta.validation.jakarta.validation-api'

javac.source: 1.8
javac.target: 1.8

# We need JUnit and Mockito to resolve the test cases at runtime. 
# Other runtime dependencies should be added as necessary
-runbundles: \
	org.apache.felix.http.servlet-api;version='[1.1.2,1.1.3)',\
	org.apache.servicemix.bundles.junit;version='[4.12.0,4.12.1)',\
	org.mockito.mockito-core;version='[1.10.19,1.10.20)',\
	org.objenesis;version='[2.2.0,2.2.1)',\
	org.osgi.util.function;version='[1.1.0,1.1.1)',\
	org.osgi.util.promise;version='[1.1.0,1.1.1)',\
	org.osgi.util.pushstream;version='[1.0.0,1.0.1)',\
	org.gecko.rest.jersey.httpwhiteboard.tests;version=snapshot,\
	org.gecko.rest.jersey.api;version=snapshot,\
	org.gecko.rest.jersey.impl;version=snapshot,\
	org.gecko.util.test.common;version='[1.3.2,1.3.3)',\
	com.sun.activation.javax.activation;version='[1.2.0,1.2.1)',\
	jakarta.activation-api;version='[1.2.1,1.2.2)',\
	jakarta.annotation-api;version='[1.3.5,1.3.6)',\
	jakarta.validation.jakarta.validation-api;version='[2.0.2,2.0.3)',\
	jakarta.ws.rs-api;version='[2.1.6,2.1.7)',\
	jakarta.xml.bind-api;version='[2.3.3,2.3.4)',\
	javassist;version='[3.27.0,3.27.1)',\
	org.apache.felix.configadmin;version='[1.9.18,1.9.19)',\
	org.apache.felix.http.jetty;version='[4.1.2,4.1.3)',\
	org.apache.felix.scr;version='[2.1.24,2.1.25)',\
	org.glassfish.hk2.api;version='[2.6.1,2.6.2)',\
	org.glassfish.hk2.external.aopalliance-repackaged;version='[2.6.1,2.6.2)',\
	org.glassfish.hk2.external.jakarta.inject;version='[2.6.1,2.6.2)',\
	org.glassfish.hk2.locator;version='[2.6.1,2.6.2)',\
	org.glassfish.hk2.osgi-resource-locator;version='[1.0.3,1.0.4)',\
	org.glassfish.hk2.utils;version='[2.6.1,2.6.2)',\
	org.glassfish.jersey.containers.jersey-container-servlet-core;version='[2.32.0,2.32.1)',\
	org.glassfish.jersey.core.jersey-client;version='[2.32.0,2.32.1)',\
	org.glassfish.jersey.core.jersey-common;version='[2.32.0,2.32.1)',\
	org.glassfish.jersey.core.jersey-server;version='[2.32.0,2.32.1)',\
	org.glassfish.jersey.inject.jersey-hk2;version='[2.32.0,2.32.1)'

Bundle-Version: 1.1.3.SNAPSHOT
Bundle-Copyright: Data In Motion Consulting
Bundle-License: Eclipse Public License v1.0
Bundle-Name: Jersey JaxRS Whiteboard HTTP Whiteboard based integration test
Bundle-Description: Jersey OSGi JaxRs Whiteboard HTTP Whiteboard based  integration tests
Bundle-Vendor: Data In Motion Consulting GmbH
Private-Package: \
	org.gecko.rest.jersey.httpwhiteboard.tests,\
	org.gecko.rest.jersey.httpwhiteboard.tests.applications,\
	org.gecko.rest.jersey.httpwhiteboard.tests.customizer,\
	org.gecko.rest.jersey.httpwhiteboard.tests.resources,\
	org.gecko.rest.jersey.tests.customizer
#Require-Capability: \
#	osgi.implementation;filter:='(&(osgi.implementation=osgi.jaxrs)(provider=jersey)(http.whiteboard=true))'

# Needed for Mockito's mocking to work
-runsystempackages.objenesis: sun.misc,sun.reflect

-resolve.effective: active;skip:='osgi.service'

# Use Felix by default
-runfw: org.apache.felix.framework;version='[6.0.3,6.0.3]'
-runvm: -ea
-runee: JavaSE-1.8
-runproperties: \
	org.osgi.service.http.port=0,\
	felix.log.level=4
